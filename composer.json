{
	"type": "project",
	"license": "proprietary",
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"php": ">=8.5",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"doctrine/dbal": "^4.4.1",
		"doctrine/doctrine-bundle": "^3.2.2",
		"doctrine/doctrine-migrations-bundle": "^4.0.0",
		"doctrine/orm": "^3.6.1",
		"gesdinet/jwt-refresh-token-bundle": "^2.0.0-beta",
		"lexik/jwt-authentication-bundle": "^v3.2.0",
		"nelmio/api-doc-bundle": "^v5.9.2",
		"nelmio/cors-bundle": "^2.6.0",
		"symfony/asset": ">=8",
		"symfony/console": ">=8.0.3",
		"symfony/dotenv": ">=8",
		"symfony/flex": "^v2.10.0",
		"symfony/framework-bundle": ">=8.0.3",
		"symfony/google-mailer": "8.0.*",
		"symfony/http-client": ">=8.0.3",
		"symfony/mailer": ">=8.0.3",
		"symfony/runtime": ">=8.0.1",
		"symfony/security-bundle": ">=8",
		"symfony/twig-bundle": ">=8.0.3",
		"symfony/uid": ">=8",
		"symfony/validator": ">=8.0.3",
		"symfony/yaml": ">=8.0.1",
		"symfonycasts/reset-password-bundle": "^v1.24.0",
		"twig/extra-bundle": "^v3.22.2",
		"twig/twig": "^v3.22.2",
		"zircote/swagger-php": "^5.7.7"
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true,
			"symfony/flex": true,
			"symfony/runtime": true
		},
		"bump-after-update": true,
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	},
	"replace": {
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-iconv": "*",
		"symfony/polyfill-php72": "*",
		"symfony/polyfill-php73": "*",
		"symfony/polyfill-php74": "*",
		"symfony/polyfill-php80": "*",
		"symfony/polyfill-php81": "*",
		"symfony/polyfill-php82": "*"
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd"
		},
		"post-install-cmd": [
			"@auto-scripts"
		],
		"post-update-cmd": [
			"@auto-scripts"
		]
	},
	"conflict": {
		"symfony/symfony": "*"
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "8.0.*"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^12.5.4",
		"symfony/maker-bundle": "^v1.65.1",
		"symfony/stopwatch": ">=8",
		"symfony/web-profiler-bundle": ">=8.0.3"
	}
}
